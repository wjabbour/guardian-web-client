{
  "name": "website-builder",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/lab": "^5.0.0-alpha.161",
    "@mui/material": "^5.15.6",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.5",
    "guardian-common": "file:../common",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "sass": "^1.70.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "cross-env": "^7.0.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "deploy": "npm run deploy-cannon && npm run deploy-gpc81",
    "deploy-cannon": "cross-env AWS_PROFILE=cannon npm run build && aws s3 sync build s3://cannonemployeestore.com/ --delete --size-only && aws cloudfront create-invalidation --distribution-id E14D6FK2VK66GQ --paths \"/*\"",
    "deploy-gpc81": "npm run build && aws s3 sync build s3://gpc81.com/ --delete --size-only && aws cloudfront create-invalidation --distribution-id E1R5RLNLHZTA8G --paths \"/*\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
